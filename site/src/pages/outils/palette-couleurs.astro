  // Setup copy functionality
  document.querySelectorAll('.copy-color').forEach(btn => {
    btn.addEventListener('click', () => {
      navigator.clipboard.writeText(btn.dataset.color);
      btn.textContent = '‚úì';
      setTimeout(() => btn.textContent = 'üìã', 1000);
    });
  });
}

function showResults() {
  document.getElementById('palette-results').classList.remove('hidden');
  generateUsageTips();
  generatePaintReferences();
  generateInspiration();
}

function generateUsageTips() {
  const tips = [
    `Utilise **${getColorName(currentPalette[0])}** comme couleur principale sur un mur accent`,
    `**${getColorName(currentPalette[1])}** sera parfait pour les coussins et textiles`,
    `Peins les boiseries en **${getColorName(currentPalette[2])}** pour une harmonie douce`,
    `Ajoute des touches de **${getColorName(currentPalette[3])}** avec des accessoires`,
    `**${getColorName(currentPalette[4])}** s'utilise id√©alement pour les murs neutres`
  ];
  
  document.getElementById('usage-tips').innerHTML = tips.map(tip => `
    <div class="flex items-start">
      <span class="text-green-500 mr-3 mt-1">üí°</span>
      <span class="text-sm" style="color: #8B8B8B;">${tip}</span>
    </div>
  `).join('');
}

function generatePaintReferences() {
  const references = [];
  
  Object.entries(paintReferences).forEach(([brand, colors]) => {
    currentPalette.slice(0, 3).forEach(color => {
      if (colors[color]) {
        references.push({ brand, color, name: colors[color] });
      }
    });
  });
  
  if (references.length === 0) {
    references.push(
      { brand: 'Castorama', color: currentPalette[0], name: 'Teinte sur mesure' },
      { brand: 'Leroy Merlin', color: currentPalette[1], name: 'Teinte personnalis√©e' },
      { brand: 'Dulux Valentine', color: currentPalette[2], name: 'Couleur sur mesure' }
    );
  }
  
  document.getElementById('paint-references').innerHTML = references.map(ref => `
    <div class="flex items-center space-x-4">
