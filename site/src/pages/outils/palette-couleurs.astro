  }
};

let currentPalette = [];

// Initialize
document.addEventListener('DOMContentLoaded', () => {
  setupMethodSelection();
  setupColorPicker();
  setupPredefinedColors();
  setupHarmonySelection();
  setupPaletteGeneration();
  
