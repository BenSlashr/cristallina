  });
  
  document.getElementById('share-palette').addEventListener('click', () => {
    const shareText = `D√©couvre ma palette d√©co personnalis√©e sur Cristallina.fr ! üé® ${currentPalette.join(' ')}`;
    if (navigator.share) {
