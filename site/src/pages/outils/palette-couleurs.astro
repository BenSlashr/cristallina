  generatePalette();
});

function setupMethodSelection() {
  document.querySelectorAll('.method-option').forEach(option => {
    option.addEventListener('click', () => {
      document.querySelectorAll('.method-option').forEach(opt => opt.classList.remove('active'));
      option.classList.add('active');
      
      const method = option.dataset.method;
      document.querySelectorAll('.method-content').forEach(content => content.classList.remove('active'));
      document.getElementById(`${method}-method`).classList.add('active');
    });
  });
}

function setupColorPicker() {
  const colorPicker = document.getElementById('color-picker');
  const colorPreview = document.getElementById('color-preview');
  
  colorPicker.addEventListener('change', (e) => {
    const color = e.target.value;
    colorPreview.style.backgroundColor = color;
    generatePalette();
  });
}

function setupPredefinedColors() {
  document.querySelectorAll('.predefined-color').forEach(colorDiv => {
    colorDiv.addEventListener('click', () => {
      const color = colorDiv.dataset.color;
      document.getElementById('color-picker').value = color;
      document.getElementById('color-preview').style.backgroundColor = color;
      
      document.querySelectorAll('.predefined-color').forEach(div => div.classList.remove('selected'));
      colorDiv.classList.add('selected');
      
      generatePalette();
    });
  });
}

function setupHarmonySelection() {
  document.querySelectorAll('input[name="harmony"]').forEach(radio => {
    radio.addEventListener('change', generatePalette);
  });
}

function setupPaletteGeneration() {
  document.getElementById('generate-palette').addEventListener('click', generatePalette);
}

function generatePalette() {
  const baseColor = document.getElementById('color-picker').value;
  const harmonyType = document.querySelector('input[name="harmony"]:checked').value;
  
  // Convert to HSL
  const [h, s, l] = hexToHsl(baseColor);
  
  // Generate harmony
  const hslColors = colorHarmonies[harmonyType](h, s, l);
  currentPalette = hslColors.map(([h, s, l]) => hslToHex(h, s, l));
  
  displayPalette(currentPalette);
  showResults();
}

function displayPalette(colors) {
  const container = document.getElementById('palette-colors');
  const detailsContainer = document.getElementById('color-details');
  
  container.innerHTML = colors.map((color, index) => `
    <div class="palette-color">
